{
    "root": true,
    "extends": [
        "html-validate:recommended",
        "html-validate:document"
    ],
    "elements": [
        "html5",
        {
            "th": {
                "inherit": "th",
                "requiredAttributes": [ ]
            }
        }
    ],
    "rules": {
        "svg-focusable": "off",
        "no-raw-characters": [
                "error",
                {
                    "relaxed": true
                }
        ],
        "require-sri": "off",
        "void-style": "off",
        "no-trailing-whitespace": "off",
        "prefer-tbody": "off"
    }
}
